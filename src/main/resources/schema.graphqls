type Query {
    bookById(id: ID): Book
    arcadeGameById(id: ID): ArcadeGame
    arcadeGames(match : String):[ArcadeGame!]!
}

type ArcadeGame {
    id: ID!
    name: String
    amountOfPlayers: String

}


type Book {
    id: ID
    name: String
    pageCount: Int
    author: Author
}


type Author {
    id: ID
    firstName: String
    lastName: String
}